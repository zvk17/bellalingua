<main class="container z-depth-2">
<div id="feedback-list">
    <div class="row">
        <div class="col s12">

            <div class="row">
                <div class="col s12"><h1 class="center-align">{{title}}</h1></div>
            </div>
            <div class="row">
                <div class="col s12">
                    <table>
                        <thead>
                          <tr>
                              <th>_id</th>
                              <th>Ime</th>
                              <th>Email</th>
                              <th>Text</th>
                              <th>Datum i vreme</th>
                          </tr>
                        </thead>

                        <tbody>
                          {{#each list}}
                          <tr>
                            <td>{{_id}}</td>
                            <td>{{name}}</td>
                            <td>{{email}}</td>
                            <td>{{text}}</td>
                            <td>{{timestamp}}</td>
                          </tr>
                          {{/each}}
                        </tbody>

                      </table>
                </div>
            </div>

        </div>
    </div>
</div>
</main>
